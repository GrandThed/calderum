# Firebase Configuration Example
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# Web Configuration
WEB_API_KEY=your_web_api_key_here
WEB_APP_ID=your_web_app_id_here
WEB_MESSAGING_SENDER_ID=your_sender_id_here
WEB_PROJECT_ID=your_project_id_here
WEB_AUTH_DOMAIN=your_auth_domain_here
WEB_STORAGE_BUCKET=your_storage_bucket_here
WEB_MEASUREMENT_ID=your_measurement_id_here

# Android Configuration
ANDROID_API_KEY=your_android_api_key_here
ANDROID_APP_ID=your_android_app_id_here

# iOS Configuration  
IOS_API_KEY=your_ios_api_key_here
IOS_APP_ID=your_ios_app_id_here
IOS_BUNDLE_ID=your_ios_bundle_id_here

# Windows Configuration
WINDOWS_API_KEY=your_windows_api_key_here
WINDOWS_APP_ID=your_windows_app_id_here
WINDOWS_MEASUREMENT_ID=your_windows_measurement_id_here